{
    "name": "innmind/cli",
    "type": "library",
    "description": "Library to build CLI tools",
    "keywords": ["cli"],
    "homepage": "http://github.com/Innmind/CLI",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/CLI/issues"
    },
    "require": {
        "php": "~7.4",
        "innmind/stream": "~2.0",
        "innmind/immutable": "~3.6",
        "innmind/url": "~3.0",
        "innmind/operating-system": "~2.0",
        "innmind/stack-trace": "~3.0",
        "symfony/dotenv": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\CLI\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\CLI\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.4",
        "innmind/black-box": "^4.10"
    }
}
