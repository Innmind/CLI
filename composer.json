{
    "name": "innmind/cli",
    "type": "library",
    "description": "Library to build CLI tools",
    "keywords": ["cli"],
    "homepage": "http://github.com/Innmind/CLI",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/CLI/issues"
    },
    "require": {
        "php": "~8.4",
        "innmind/io": "~4.0",
        "innmind/immutable": "~6.0",
        "innmind/url": "~5.0",
        "innmind/operating-system": "~7.0",
        "innmind/stack-trace": "~5.0",
        "innmind/validation": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\CLI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\CLI\\": "tests/"
        }
    },
    "require-dev": {
        "innmind/static-analysis": "~1.3",
        "innmind/black-box": "^6.4.1",
        "innmind/coding-standard": "~2.0"
    }
}
