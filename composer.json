{
    "name": "innmind/cli",
    "type": "library",
    "description": "Library to build CLI tools",
    "keywords": ["cli"],
    "homepage": "http://github.com/Innmind/CLI",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/CLI/issues"
    },
    "require": {
        "php": "~8.1",
        "innmind/stream": "~3.0",
        "innmind/immutable": "~4.0",
        "innmind/url": "~4.0",
        "innmind/operating-system": "~3.0",
        "innmind/stack-trace": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\CLI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\CLI\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.4",
        "innmind/black-box": "^4.10",
        "innmind/coding-standard": "~2.0"
    }
}
